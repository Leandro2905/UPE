    GROUPBOX "מצב תאימות", IDC_COMPATGROUP, 7, 41, 210, 49
    CHECKBOX "הפעל תוכנית זו במצב תאימות עבור:", IDC_CHKRUNCOMPATIBILITY, 18, 57, 188, 10, BS_AUTOCHECKBOX | WS_TABSTOP

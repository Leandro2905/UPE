    IDS_TOOLTIP_NEW, "Nou"
    IDS_TOOLTIP_OPEN, "Deschide"
    IDS_TOOLTIP_SAVE, "Păstrează"
    IDS_TOOLTIP_EDIT_GLYPH, "Editează glifă"

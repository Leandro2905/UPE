        MENUITEM "&ЧЗВ за изследователя...", ID_EXPLORER_FAQ
        MENUITEM "&За изследователя...", ID_ABOUT_EXPLORER
        MENUITEM "За &РеактОС...", ID_ABOUT_WINDOWS
    END
END


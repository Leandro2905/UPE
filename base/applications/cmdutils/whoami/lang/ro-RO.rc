    IDS_GROU_HEADER "INFORMAȚII DE GRUP"
    IDS_PRIV_HEADER "INFORMAȚII DE PRIVILEGIU"
    IDS_COL_USER_NAME "Nume utilizator"
    IDS_COL_GROUP_NAME "Nume grup"
    IDS_COL_TYPE "Tip"

LANGUAGE LANG_ITALIAN, SUBLANG_NEUTRAL

STRINGTABLE
BEGIN
    IDS_HELP "Confronta il contenuto di due file o gruppi di file.\n\n\
COMP [/L] [/A] [file1] [file2]\n\n\

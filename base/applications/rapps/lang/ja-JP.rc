    PUSHBUTTON "デフォルト", IDC_DEFAULT_SETTINGS, 8, 195, 60, 14
    DEFPUSHBUTTON "OK", IDOK, 116, 195, 60, 14
    PUSHBUTTON "キャンセル", IDCANCEL, 181, 195, 60, 14
END


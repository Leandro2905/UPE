LANGUAGE LANG_ITALIAN, SUBLANG_NEUTRAL

IDR_MAINMENU MENU
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "&Apri...\tCtrl+O", IDM_OPEN_FILE
        MENUITEM "&Chiudi\tCtrl+W", IDM_CLOSE_FILE
        MENUITEM SEPARATOR
        MENUITEM "&Esci", IDM_EXIT
    END
    POPUP "&Play"
    BEGIN
        MENUITEM "Play/&Pausa\tCtrl+P", IDC_PLAY
        MENUITEM "St&op\tCtrl+S", IDC_STOP
        MENUITEM SEPARATOR
        MENUITEM "Ripet&i\tCtrl+T", IDM_REPEAT
    END
    POPUP "&Visualizza"
    BEGIN
        MENUITEM "&Modalità Finestra-Singola", IDM_SWITCHVIEW
    END
    POPUP "&Dispositivo"
    BEGIN
        MENUITEM SEPARATOR
        MENUITEM "&Proprietà", IDM_DEVPROPS
        MENUITEM SEPARATOR
        MENUITEM "&Controllo Volume", IDM_VOLUMECTL
    END
    POPUP "&Aiuto"
    BEGIN
        MENUITEM "&Informazioni", IDM_ABOUT
    END
END

ID_ACCELERATORS ACCELERATORS
BEGIN
    "^O", IDM_OPEN_FILE
    "^W", IDM_CLOSE_FILE
    "^P", IDC_PLAY
    "^S", IDC_STOP
    "^T", IDM_REPEAT
END


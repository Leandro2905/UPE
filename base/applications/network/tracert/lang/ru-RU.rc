
LANGUAGE LANG_RUSSIAN, SUBLANG_DEFAULT

STRINGTABLE
BEGIN
    IDS_USAGE "\r\n\
Использование: tracert [-d] [-h максЧисло] [-j списокУзлов]\r\n\
                       [-w таймаут] конечноеИмя\r\n\r\n\
Параметры:\r\n\
